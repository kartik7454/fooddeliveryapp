generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = process.env.MONGO
}



model User {
  id            String    @id @default(auto()) @map("_id") @db.ObjectId
  email         String?   @unique
  
    
  name          String?
  postalcode    Int?
  address    String?
  phonenumber    Int?
 admin         Boolean?
  image         String?
  createdAt     DateTime  @default(now())
  updatedAt     DateTime  @updatedAt
   
  
}

